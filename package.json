{
  "name": "layaair-tape",
  "main": "./dist/tape.js",
  "version": "3.5.2",
  "description": "A layaair game library",
  "scripts": {
    "start": "webpack --watch",
    "build-dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --mode production && webpack --config webpack.wxrank.js --mode production",
    "build-test-fb": "node ./bin/build.js --platform facebook --input ./test/bin/ --output ./build/  --force",
    "build-test-h5": "node ./bin/build.js --platform h5 --output ./build/ --input ./examples/demo/bin/ --force",
    "build-test-qq": "node ./bin/build.js --platform qq --output ./build/ --input ./examples/demo/bin/ --zip --min --minpng",
    "build-test-wx": "node ./bin/build.js --platform wechat --input ./examples/demo/bin/ --output ./build/ --min --env prod --publish"
  },
  "author": "liangmayong",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoopschat/layaair-tape.git"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "keywords": [
    "layaair",
    "tape",
    "layaair-tape"
  ],
  "bin": {
    "tape": "./bin/build.js"
  },
  "files": [
    "tpl",
    "bin",
    "dist",
    "include"
  ],
  "dependencies": {
    "cheerio": "*",
    "del": "*",
    "gulp": ">=4.0.0",
    "gulp-zip": "*",
    "gulp-cheerio": "*",
    "gulp-concat": "*",
    "gulp-downloader": "*",
    "gulp-pngquant": "*",
    "gulp-uglify": "*",
    "gulp-replace": "*",
    "minimist": "*",
    "through2": "*",
    "iconv-lite": "*",
    "exec-sh": "*"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "es6-promise": "^4.2.4",
    "fs-extra": "^6.0.1",
    "string-replace-loader": "^2.1.1",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  }
}
